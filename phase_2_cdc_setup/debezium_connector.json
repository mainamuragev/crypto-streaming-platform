{
    "name": "crypto-postgres-connector",
    "config": {
        "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
        "database.hostname": "postgres",
        "database.port": "5432",
        "database.user": "crypto_user",
        "database.password": "${POSTGRES_PASSWORD}",
        "database.dbname": "crypto_db",
        "database.server.name": "crypto_cdc",
        "table.include.list": "public.crypto_ticker_24hr",
        "plugin.name": "pgoutput",
        "slot.name": "crypto_debezium_slot",
        "publication.name": "crypto_publication"
    }
}
